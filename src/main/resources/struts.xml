<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
    <package name="backstage" namespace="/backstage" extends="struts-default">
        <action name="index" class="loginAction" method="CheckForntLogin">
            <result name="success">/WEB-INF/backstage/index.jsp</result>
        </action>
    </package>
    <package name="reception" namespace="/reception" extends="struts-default">
        <action name="*">
            <result>/WEB-INF/reception/{1}.jsp</result>
        </action>
    </package>
</struts>